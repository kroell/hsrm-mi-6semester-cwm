<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
	<bean id="qslRequestWorkflow.workflowBootstrap" parent="workflowDeployer">
	
		<!-- Registration of new workflow definition -->
		<property name="workflowDefinitions">
			<list>
				<props>
					<prop key="engineId">jbpm</prop>
					<prop key="location">alfresco/module/QslRequestWorkflow/processDefinition/qslRequestWorkflow_processdefinition.xml</prop>
					<prop key="mimetype">text/xml</prop>
					<prop key="redeploy">true</prop>
				</props>
			</list>
		</property>

		<!-- Registration of new models -->
		<property name="models">
			<list>
				<value>alfresco/module/QslRequestWorkflow/model/qslRequestWorkflowModel.xml</value>
			</list>
		</property>
		
		<!-- Registration of new labels -->
		<property name="labels">
			<list>
				<value>alfresco/module/QslRequestWorkflow/messages/qsl-request-workflow-model</value>
			</list>
		</property>
		
	</bean>
</beans>
